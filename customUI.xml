          <separator id="MySep3.1"/>
          <!--<button id="MyOutput" size="large" label="出力&#xD;&#xA;" imageMso="CalculateNow" onAction="MyOutput_OnClick" />-->
          <button id="MyOutput" size="large" label="出力&#xD;&#xA;" imageMso="ExportExcel" onAction="MyOutput_OnClick" />
          <button id="MyOutputConfig" size="large" label="出力設定&#xD;&#xA;" imageMso="CalculatedYesNo" onAction="MyOutputConfig_OnClick" />
        </group>
        <group id="MyGroup4" label="断熱工事">
          <button id="MyButton4.1" size="large" label="ダミー&#xD;&#xA;"/>
        </group>
        <group id="MyGroup5" label="足場工事">
          <button id="MyButton5.1" size="large" label="ダミー&#xD;&#xA;"/>
        </group>
      </tab>
    </tabs>
