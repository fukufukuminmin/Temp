<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="ribbonLoaded">
  <ribbon startFromScratch="false">
    <tabs>
      <tab id="MyTab1" label="積算ソフト" insertBeforeQ="TabHome">
        <group id="MyGroup1" label="ヘルプ">
          <button id="MyHelpNormal" size="large" label="ヘルプ&#xD;&#xA;(通常)" imageMso="Help" onAction="MyHelpNormal_OnClick" />
          <button id="MyHelpOnTop" size="large" label="ヘルプ&#xD;&#xA;(最前面)" imageMso="Help" onAction="MyHelpOnTop_OnClick" />
          <separator id="MySep1.1"/>
          <button id="MyVersion" size="large" label="バー&#xD;&#xA;ジョン" imageMso="Info" onAction="MyVersion_OnClick" />
          <button id="MyInformation" size="large" label="積算情報&#xD;&#xA;" imageMso="Info" onAction="MyInformation_OnClick" />
        </group>
        <group id="MyGroup2" label="データベース">
